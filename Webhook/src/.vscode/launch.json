    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "DEBUG": "true",
                "JSONLOG": "false",
                "HOST": "0.0.0.0",
                "PORT": "9001",
                "TLSCERT": "server.crt",
                "TLSKEY": "server.key",
                "SOURCE_SECRET_NAME": "regcred",
                "SOURCE_SECRET_NAMESPACE": "sample",
                "TARGET_SERVICE_ACCOUNT": "default"
            },
            "args": []
        }
    ]
